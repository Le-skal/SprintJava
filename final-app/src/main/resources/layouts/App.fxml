<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox 
    xmlns="http://javafx.com/javafx/8" 
    xmlns:fx="http://javafx.com/fxml/1" 
    spacing="10" 
    alignment="CENTER"
>
    <Label fx:id="messageLabel" text="Menu Principal !" />
    <ComboBox fx:id="fontComboBox" promptText="Select Font" />
    <Button fx:id="button_1" text="Lister les utilisateurs"/>
    <Button fx:id="button_2" text="Ajouter un utilisateur"/>
    <Button fx:id="button_3" text="Supprimer un utilisateur"/>
    <Button fx:id="button_4" text="Modifier un utilisateur"/>
    <Button fx:id="button_5" text="Exporter en CSV"/>
    <Button fx:id="button_0" text="Quiter"/>
</VBox>
<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox 
    xmlns="http://javafx.com/javafx/8" 
    xmlns:fx="http://javafx.com/fxml/1" 
    spacing="10" 
    alignment="CENTER"
>
    <Label fx:id="updater_messageLabel" text="Modifier des Utilisateurs !"/>
    <TextField fx:id="idField" promptText="Entrez l'id"/>
    <TextField fx:id="nameField" promptText="Entrez le nom"/>
    <TextField fx:id="emailField" promptText="Entrez l'email"/>
    <Button fx:id="updater_button_1" text="Updater"/>
    <Button fx:id="updater_button_0" text="Quiter"/>
</VBox>
